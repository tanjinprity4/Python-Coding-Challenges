string_numbers = input("Enter distance of houses for array: ")
    int_array = list(map(int, string_numbers))
    int_array_length = len(int_array)
    int_counter = 0
    bool_distance = True
    while int_counter < int_array_length:
        int_x = int_array[int_counter]
        print(str(int_x))
        if  int_x == (int_array_length-1) - int_counter:
            bool_distance = True
        else:
            bool_distance = False
            break
        int_counter += 1
    if bool_distance:
        print("Everyone can party!")
    else:
        print("No party!")
      
if __name__ == "__main__":
    run()
